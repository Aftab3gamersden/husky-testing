{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "target": "es2017",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
    "exclude": ["__tests__/**/*-test.ts"],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "assets/icons/__tests__/Iconstest.js", "assets/icons/__tests__/IconsTest-test.js"
  ]
}
